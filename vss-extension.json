{
    "manifestVersion": 1,
    "id": "one-signal-azure-dev-ops-task-ext",
    "name": "OneSignal Rest API for Azure DevOps",
    "version": "0.0.13",
    "publisher": "DavutEngin",
    "icons": {
        "default": "images/icon.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
